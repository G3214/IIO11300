<Window x:Class="H1MediaPlayer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:H1MediaPlayer"
        mc:Ignorable="d"
        Title="My Finest Media Player" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnPlay" Content="HoyoHoyo" HorizontalAlignment="Left" Margin="120,10,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="btnPause" Content="Hoyo?" HorizontalAlignment="Left" Margin="205,10,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="btnStop" Content="Ei HoyoHoyo" HorizontalAlignment="Left" Margin="290,10,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
