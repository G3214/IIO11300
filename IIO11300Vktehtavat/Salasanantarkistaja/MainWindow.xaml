<Window x:Class="Salasanantarkistaja.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Salasanantarkistaja"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Label>Tarkistettava salasana</Label>
            <TextBox x:Name="tbInput">...</TextBox>
            <Label x:Name="lCharacters">...</Label>
            <Label x:Name="lBigLetters">...</Label>
            <Label x:Name="lSmallLetters">...</Label>
            <Label x:Name="lNumberes">...</Label>
            <Label x:Name="lSpecial">...</Label>


            <Label x:Name="lOutPut" Background="Gray" FontSize="80">anna salasana</Label>
        </StackPanel>
        
    </Grid>
</Window>
